generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model doctors {
  id    Int    @id
  name  String
  email String
}

model users {
  id          String    @id
  name        String
  email       String?
  password    String
  age         Int
  phoneNumber String    @unique(map: "sqlite_autoindex_users_2")
  address     String
  gender      String
  createdAt   DateTime? @default(now())
}
